<li class="item">
  <span>
    <% reviewee = evaluation.reviewee %>
    <%= link_to profile_user_path(reviewee) do %>
      <%= attachment_image_tag reviewee, :icon_image, :fill, 40, 40, fallback: "no-icon.png", size:"40x40", class: "face_icon" %>
      <%= reviewee.nickname %>
    <% end %>
    <div class="review_stars" data-score="<%= evaluation.stars %>"></div>
    <%= evaluation.comment %>
  </span>
</li>
