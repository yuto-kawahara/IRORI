<div class="search_form_area">
  <%= form_with url: evaluations_search_path, method: :get, local: true do |f| %>
    <%= f.text_field :content, autocomplete: "off", autofocus: true, placeholder: "\uf002 ユーザー検索", id: "user_search_form", value: @content %>
    <ul class="search_result"></ul>
  <% end %>
</div>