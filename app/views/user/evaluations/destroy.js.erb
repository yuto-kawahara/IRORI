$(".item_<%= @evaluation.id %>").remove();
var review_stars = $('.review_stars');
if (review_stars != null) {
  review_stars.empty();
  review_stars.raty({
    readOnly: true,
    score: function() {
      return review_stars.attr('data-score');
    },
    path: '/assets/'
  });
}
var cover = $('#cover');
cover.removeClass("hidden");