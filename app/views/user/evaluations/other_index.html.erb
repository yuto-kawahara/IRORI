<div id="wrapper">
  <div class="head_link">
    <h2 class="head_link__title">卓報告</h2>
  </div>
  <ul class="tab">
    <li class="tab__item">
      <%= link_to user_evaluations_path(current_user), class: "tab__link" do %>
        自分の報告
      <% end %>
    </li>
    <li class="tab__item">
      <%= link_to other_user_evaluations_path, class: "tab__link" do %>
        みんなの報告
      <% end %>
    </li>
  </ul>
  <div class="tab_content">
    <%= render 'index', evaluations: @evaluations %>
  </div>
</div>
