<div id="wrapper">
  <div class="inner">
    <div class="head_link">
      <h2>通知</h2>
    </div>
    <% if @notifications.present? %>
      <ul class="list">
        <%= render @notifications %>
      </ul>
      <div id="modal"></div>
      <%= paginate @notifications %>
      <%= render 'layouts/loader' %>
    <% else %>
      <p>通知はありません</p>
    <% end %>
  </div>
</div>