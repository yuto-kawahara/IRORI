<div id="wrapper">
  <div class="head_link">
    <span class="head_link__inner">
      <%= link_to recruits_path, class: "btn btn__back" do %>
        <i class="fas fa-arrow-left"></i>
      <% end %>
      <h2 class="head_link__title">募集カレンダー</h2>
    </span>
  </div>
  <div class="area area__recruit_list">
    <div class="user_recruit_list">
      <%= render 'user/recruits/list', recruits: @recruits %>
    </div>
  </div>
</div>
